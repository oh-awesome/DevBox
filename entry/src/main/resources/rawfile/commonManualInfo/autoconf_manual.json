      {
        "paramName": "-I, --include=DIR",
        "description": "append directory DIR to search path"
      }
    ],
    "examples": [
      "autoheader -V",
      "autoheader --help"
    ],
    "showDetails": false
  },
  {
    "name": "autom4te",
    "description": "在文件上运行GNU M4，避免无用运行的工具",
    "syntax": "autom4te [OPTION]... [FILES]",
    "options": [
      {
        "paramName": "-h, --help",
        "description": "print this help, then exit"
      },
      {
        "paramName": "-V, --version",
        "description": "print version number, then exit"
      },
      {
        "paramName": "-v, --verbose",
        "description": "verbosely report processing"
      },
      {
        "paramName": "-d, --debug",
        "description": "don't remove temporary files"
      },
      {
        "paramName": "-o, --output=FILE",
        "description": "save output in FILE (defaults to '-', stdout)"
      },
      {
        "paramName": "-f, --force",
        "description": "don't rely on cached values"
      },
      {
        "paramName": "-W, --warnings=CATEGORY",
        "description": "report the warnings falling in CATEGORY (comma-separated list accepted)"
      },
      {
        "paramName": "-l, --language=LANG",
        "description": "specify the set of M4 macros to use"
      },
      {
        "paramName": "-C, --cache=DIRECTORY",
        "description": "preserve results for future runs in DIRECTORY"
      },
      {
        "paramName": "--no-cache",
        "description": "disable the cache"
      },
      {
        "paramName": "-m, --mode=OCTAL",
        "description": "change the non trace output file mode (0666)"
      },
      {
        "paramName": "-M, --melt",
        "description": "don't use M4 frozen files"
      },
      {
        "paramName": "-B, --prepend-include=DIR",
        "description": "prepend directory DIR to search path"
      },
      {
        "paramName": "-I, --include=DIR",
        "description": "append directory DIR to search path"
      },
      {
        "paramName": "-t, --trace=MACRO[:FORMAT]",
        "description": "report the MACRO invocations"
      },
      {
        "paramName": "-p, --preselect=MACRO",
        "description": "prepare to trace MACRO in a future run"
      },
      {
        "paramName": "-F, --freeze",
        "description": "produce an M4 frozen state file for FILES"
      }
    ],
    "examples": [
      "autom4te --version",
      "autom4te -h"
    ],
    "showDetails": false
  },
