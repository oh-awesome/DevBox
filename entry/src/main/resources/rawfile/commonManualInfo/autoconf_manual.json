      },
      {
        "paramName": "-d, --debug",
        "description": "don't remove temporary files"
      },
      {
        "paramName": "-f, --force",
        "description": "consider all files obsolete"
      },
      {
        "paramName": "-W, --warnings=CATEGORY",
        "description": "report the warnings falling in CATEGORY (comma-separated list accepted)"
      },
      {
        "paramName": "-B, --prepend-include=DIR",
        "description": "prepend directory DIR to search path"
