[
  {
    "name": "autoconf",
    "description": "从模板文件生成配置脚本的工具",
    "syntax": "autoconf [OPTION]... [TEMPLATE-FILE]",
    "options": [
      {
        "paramName": "-h, --help",
        "description": "print this help, then exit"
      },
      {
        "paramName": "-V, --version",
        "description": "print version number, then exit"
      },
      {
        "paramName": "-v, --verbose",
        "description": "verbosely report processing"
      },
      {
        "paramName": "-d, --debug",
        "description": "don't remove temporary files"
      },
      {
        "paramName": "-f, --force",
        "description": "consider all files obsolete"
      },
      {
        "paramName": "-o, --output=FILE",
        "description": "save output in FILE (stdout is the default)"
      },
      {
        "paramName": "-W, --warnings=CATEGORY",
        "description": "report the warnings falling in CATEGORY (comma-separated list accepted)"
      },
      {
        "paramName": "-B, --prepend-include=DIR",
        "description": "prepend directory DIR to search path"
      },
      {
        "paramName": "-I, --include=DIR",
        "description": "append directory DIR to search path"
      },
      {
        "paramName": "-t, --trace=MACRO[:FORMAT]",
        "description": "report the list of calls to MACRO"
      },
      {
        "paramName": "-i, --initialization",
        "description": "also trace Autoconf's initialization process"
      }
    ],
    "examples": [
      "autoconf -h",
      "autoconf -V"
    ],
    "showDetails": false
  },
  {
