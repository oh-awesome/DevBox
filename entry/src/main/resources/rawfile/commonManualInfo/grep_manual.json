[
  {
    "name": "egrep",
    "description": "正则表达式匹配工具（grep的扩展版本），用于查找文件中匹配扩展正则表达式的行。",
    "syntax": "egrep [-EFrivwcloqsHbhn] [-ABC NUM] [-m MAX] [-e REGEX]... [-MS PATTERN]... [-f REGFILE] [FILE]...",
    "options": [
      {
        "paramName": "-e",
        "description": "Regex to match. (May be repeated.)"
      },
      {
        "paramName": "-f",
        "description": "File listing regular expressions to match."
      },
      {
        "paramName": "-r",
        "description": "Recurse into subdirectories (defaults FILE to \".\")"
      },
      {
        "paramName": "-R",
        "description": "Recurse into subdirectories and symlinks to directories"
      },
      {
        "paramName": "-M",
        "description": "Match filename pattern (--include)"
      },
      {
        "paramName": "-S",
        "description": "Skip filename pattern (--exclude)"
      },
      {
        "paramName": "--exclude-dir=PATTERN",
        "description": "Skip directory pattern"
      },
      {
        "paramName": "-I",
        "description": "Ignore binary files"
      },
      {
        "paramName": "-A",
        "description": "Show NUM lines after match"
      },
      {
        "paramName": "-B",
        "description": "Show NUM lines before match"
      },
      {
        "paramName": "-C",
        "description": "NUM lines context (A+B)"
      },
      {
        "paramName": "-E",
        "description": "extended regex syntax"
      },
      {
        "paramName": "-F",
        "description": "fixed (literal match)"
      },
      {
        "paramName": "-a",
        "description": "always text (not binary)"
      },
      {
        "paramName": "-i",
        "description": "case insensitive"
      },
      {
        "paramName": "-m",
        "description": "match MAX many lines"
      },
      {
        "paramName": "-v",
        "description": "invert match"
      },
      {
        "paramName": "-w",
        "description": "whole word (implies -E)"
      },
      {
        "paramName": "-x",
        "description": "whole line"
      },
      {
        "paramName": "-z",
        "description": "input NUL terminated"
      },
      {
        "paramName": "-L",
        "description": "filenames with no match"
      },
      {
        "paramName": "-Z",
        "description": "output is NUL terminated"
      },
      {
        "paramName": "-c",
        "description": "count of matching lines"
      },
      {
        "paramName": "-l",
        "description": "filenames with a match"
      },
      {
        "paramName": "-o",
        "description": "only matching part"
      },
      {
        "paramName": "-q",
        "description": "quiet (errors only)"
      },
      {
        "paramName": "-s",
        "description": "silent (no error msg)"
      },
      {
        "paramName": "-H",
        "description": "force filename"
      },
      {
        "paramName": "-b",
        "description": "byte offset of match"
      },
      {
        "paramName": "-h",
        "description": "hide filename"
      },
      {
        "paramName": "-n",
        "description": "line number of match"
      }
    ],
    "examples": [
      "egrep 'pattern' file.txt",
      "egrep -i 'pattern' file1.txt file2.txt"
    ],
    "showDetails": false
  },
