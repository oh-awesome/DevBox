  {
    "name": "eqn",
    "description": "groff文档格式化系统的公式处理工具，用于在roff格式文档中排版数学公式",
    "syntax": "eqn [-CNrR] [-d xy] [-f font] [-m n] [-M dir] [-p n] [-s n] [-T name] [file ...],eqn {-v | --version},eqn --help",
    "options": [
      {
        "paramName": "--help",
        "description": "Display help information"
      },
      {
        "paramName": "-v, --version",
        "description": "Display version information"
      },
      {
        "paramName": "-C",
        "description": "Enable specific formatting option (details refer to groff documentation)"
      },
      {
        "paramName": "-N",
        "description": "Disable specific formatting option (details refer to groff documentation)"
      },
      {
        "paramName": "-r",
        "description": "Enable raw output mode (details refer to groff documentation)"
      },
      {
        "paramName": "-R",
        "description": "Disable raw output mode (details refer to groff documentation)"
      },
      {
        "paramName": "-d xy",
        "description": "Set dimension parameters for formula layout"
      },
      {
        "paramName": "-f font",
        "description": "Specify font for formula rendering"
      },
      {
        "paramName": "-m n",
        "description": "Set magnification factor for formulas"
      },
      {
        "paramName": "-M dir",
        "description": "Specify directory for macro files"
      },
      {
        "paramName": "-p n",
        "description": "Set precision parameter for formula typesetting"
      },
      {
        "paramName": "-s n",
        "description": "Set size parameter for formula elements"
      },
      {
        "paramName": "-T name",
        "description": "Specify output device or format (e.g., ps, pdf, html)"
      }
    ],
    "examples": [
      "eqn --help",
      "eqn --version",
      "eqn -T ps formula.roff > formatted.ps"
    ],
    "showDetails": false
  },
